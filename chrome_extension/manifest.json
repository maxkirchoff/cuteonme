{
	"name": "CuteOn.Me",
	"version": "1.1",
	"description": "Popup helper for CuteOn.Me Sharing",
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "CuteOn.Me",
		"default_popup": "popup.html"
	},
	"update_url": "http://www.cuteon.me/chrome/updates.xml",
	"permissions": [
		"http://*/*",
		"https://*/*",
		"tabs",
		"debugger"
	]
}